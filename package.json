{
  "name": "react-ssr-boilerplate",
  "version": "1.0.0",
  "description": "A react server side boilerplate project",
  "main": "index.js",
  "repository": "https://github.com/rafaelpizzaia/react-ssr-boilerplate.git",
  "author": "Rafael Pizzaia Coleta",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:build-server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "build:build-client": "cross-env NODE_ENV=production webpack --config webpack.client.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-server": "cross-env NODE_ENV=development webpack --config webpack.server.js --watch",
    "dev:build-client": "cross-env NODE_ENV=development webpack --config webpack.client.js --watch",
    "dev:run-server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "lint": "eslint src --ext .js --ext .jsx --no-cache",
    "stylelint": "stylelint src/**/*.scss"
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-polyfill": "^6.26.0",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-icons": "^2.2.7",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "1.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "node-sass": "^4.7.2",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.10",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-sass-guidelines": "^4.1.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.3.0",
    "webpack": "3.5.6",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "2.8.2",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  }
}
